<!doctype html>
<html>
	<head>
	
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		
		<title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>

		<style>
		{{{css}}}
		</style>
		
		<script src="https://kit.fontawesome.com/6b34f1f9a6.js" crossorigin="anonymous"></script>
		{{#if resume.google_analytics_id}}
			<!-- Global site tag (gtag.js) - Google Analytics -->
			<script async src="https://www.googletagmanager.com/gtag/js?id={{resume.google_analytics_id}}"></script>
			<script>
				window.dataLayer = window.dataLayer || [];
				function gtag(){dataLayer.push(arguments);}
				gtag('js', new Date());
				gtag('config', '{{resume.google_analytics_id}}');
			</script>
		{{/if}}
	</head>
	<body>

		<div id="resume">
			{{> basics }}
			{{> profiles }}
			{{> work }}
			{{> education }}
			{{> projects }}
			{{> skills }}
			{{> languages }}
			{{> interests }}
			{{> contact }}
		</div>

	</body>
</html>
